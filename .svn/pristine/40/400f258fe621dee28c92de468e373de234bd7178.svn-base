<ion-header mode='ios'>
    <ion-toolbar mode='ios'>
      <ion-buttons slot="start">
        <ion-back-button text="" icon="ios-arrow-back" mode='ios'></ion-back-button>
      </ion-buttons>
      <ion-title>关联项目信息</ion-title>
    </ion-toolbar>
  </ion-header>
  
<ion-content>
  <ion-grid class="dec-list">
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">项目编号：</ion-col>
      <ion-col class="right-text" size="7">{{projectCode}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">项目名称：</ion-col>
      <ion-col class="right-text" size="7">{{projectName}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">建设地点：</ion-col>
      <ion-col class="right-text" size="7">{{address}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">所属行业：</ion-col>
      <ion-col class="right-text" size="7">{{industry}}</ion-col>
    </ion-row>
    <ion-row>
      <ion-col class="left-text" size="5">环保投资额（万元）：</ion-col>
      <ion-col class="right-text" size="7">{{investmentMoney}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">审批时间：</ion-col>
      <ion-col class="right-text" size="7">{{approvalTime}}</ion-col>
    </ion-row>

  </ion-grid>



</ion-content>