<ion-header mode='ios'>
  <ion-toolbar mode='ios'>
    <ion-buttons slot="start">
      <ion-back-button text="" icon="ios-arrow-back" mode='ios'></ion-back-button>
    </ion-buttons>
    <ion-title>建设项目环评审批基础信息表(1)</ion-title>
  </ion-toolbar>
</ion-header>

<ion-content>
  <ion-grid class="dec-list">
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">项目名称：</ion-col>
      <ion-col class="right-text" size="7">{{projectName}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">项目代码：</ion-col>
      <ion-col class="right-text" size="7">{{projectNumber}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">国民经济行业类型：</ion-col>
      <ion-col class="right-text" size="7">{{countIndustryType}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="7">环境影响评价行业类别：</ion-col>
      <ion-col class="right-text" size="5">{{eviIndustryType}}</ion-col>
    </ion-row>
    <ion-row>
      <ion-col class="left-text" size="8">建设地点中心坐标经度(非线性)：</ion-col>
      <ion-col class="right-text" size="4">{{constructionSiteLon}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="8">建设地点中心坐标纬度(非线性)：</ion-col>
      <ion-col class="right-text" size="4">{{constructionSiteLat}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">建设性质：</ion-col>
      <ion-col class="right-text" size="7">{{constructionNature}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="7">环境影响评价文件类别：</ion-col>
      <ion-col class="right-text" size="5">{{eviFileType}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">总投资(万元)：</ion-col>
      <ion-col class="right-text" size="7">{{totalInvest}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">环保投资(万元)：：</ion-col>
      <ion-col class="right-text" size="7">{{enviInvest}}</ion-col>
    </ion-row>
    <ion-row>
      <ion-col class="left-text" size="5">所占比例(%)：</ion-col>
      <ion-col class="right-text" size="7">{{proportion}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">计划开工时间：</ion-col>
      <ion-col class="right-text" size="7">{{planStartTime}}</ion-col>
    </ion-row>
    <ion-row>
      <ion-col class="left-text" size="7">预计投产时间：</ion-col>
      <ion-col class="right-text" size="5">{{estimatedTime}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">建设地点：</ion-col>
      <ion-col class="right-text" size="7">{{constructionSite}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">项目建设周期(月)：</ion-col>
      <ion-col class="right-text" size="7">{{constructionCycle}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="8">现有工程排污许可证编号(改建、扩建项目)：</ion-col>
      <ion-col class="right-text" size="4">{{licenseNumber}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">项目申请类别：</ion-col>
      <ion-col class="right-text" size="7">{{peojectApplyType}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">规划环评开展情况：</ion-col>
      <ion-col class="right-text" size="7">{{isDevople}}</ion-col>
    </ion-row>
    <ion-row>
      <ion-col class="left-text" size="5">规划环评文件名：</ion-col>
      <ion-col class="right-text" size="7">{{planDocumentName}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">规划环评审查机关：</ion-col>
      <ion-col class="right-text" size="7">{{planExamAuthority}}</ion-col>
    </ion-row>
    <ion-row>
      <ion-col class="left-text" size="7">规划环评审查意见文号：</ion-col>
      <ion-col class="right-text" size="5">{{planExamNumber}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">建设内容：</ion-col>
      <ion-col class="right-text" size="7">{{constructContent}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="7">建设规模：</ion-col>
      <ion-col class="right-text" size="5">{{constructScale}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="9">建设地点坐标(线性工程)起点经度：</ion-col>
      <ion-col class="right-text" size="3">{{startLon}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">起点纬度：</ion-col>
      <ion-col class="right-text" size="7">{{startLat}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">终点经度：</ion-col>
      <ion-col class="right-text" size="7">{{endLon}}</ion-col>
    </ion-row>
    <ion-row>
      <ion-col class="left-text" size="5">终点纬度：</ion-col>
      <ion-col class="right-text" size="7">{{endLat}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">工程长度(千米)：</ion-col>
      <ion-col class="right-text" size="7">{{engineerLength}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">建设单位：</ion-col>
      <ion-col class="right-text" size="7">{{constructionUnit}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="7">建设单位通讯地址：</ion-col>
      <ion-col class="right-text" size="5">{{address}}</ion-col>
    </ion-row>
    <ion-row>
      <ion-col class="left-text" size="5">建设单位法人代表：</ion-col>
      <ion-col class="right-text" size="7">{{legalPerson}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">建设单位技术负责人：</ion-col>
      <ion-col class="right-text" size="7">{{technology}}</ion-col>
    </ion-row>

    <ion-row>
      <ion-col class="left-text" size="6">建设单位同意社会信用代码：</ion-col>
      <ion-col class="right-text" size="6">{{socialCreditCode}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
        <ion-col class="left-text" size="5">建设单位联系电话：</ion-col>
        <ion-col class="right-text" size="7">{{constructionUnitPhone}}</ion-col>
      </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">评价单位名称：</ion-col>
      <ion-col class="right-text" size="7">{{evaluatUnitName}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">评价单位通讯地址：</ion-col>
      <ion-col class="right-text" size="7">{{evaluatUnitAddress}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="7">评价单位联系电话：</ion-col>
      <ion-col class="right-text" size="5">{{evaluatUnitPhone}}</ion-col>
    </ion-row>
    <ion-row>
      <ion-col class="left-text" size="5">评价单位证书编号：</ion-col>
      <ion-col class="right-text" size="7">{{evaluatUnitNumber}}</ion-col>
    </ion-row>
  </ion-grid>
</ion-content>