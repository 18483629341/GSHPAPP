<ion-header mode='ios'>
  <ion-toolbar mode='ios'>
    <ion-buttons slot="start">
      <ion-back-button text="" icon="ios-arrow-back" mode='ios'></ion-back-button>
    </ion-buttons>
    <ion-title>诚信信息</ion-title>
  </ion-toolbar>
</ion-header>

<ion-content>
  <!-- 详情列表 -->
  <ion-grid class="dec-list">
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">时间：</ion-col>
      <ion-col class="right-text" size="7">{{time}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">监管部门：</ion-col>
      <ion-col class="right-text" size="7">{{supervisionDepartment}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">行政处理方式：</ion-col>
      <ion-col class="right-text" size="7">{{processMode}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">受到行政处罚的主要原因：</ion-col>
      <ion-col class="right-text" size="7">{{reason}}</ion-col>
    </ion-row>
    <ion-row>
      <ion-col class="left-text" size="5">处理结果：</ion-col>
      <ion-col class="right-text" size="7">{{processResult}}</ion-col>
    </ion-row>
    <!-- <ion-row align-items-center>
      <ion-col class="left-text" size="5">附件名称：</ion-col>
      <ion-col class="right-text" size="7">{{fileName}}</ion-col>
    </ion-row> -->
  </ion-grid>
</ion-content>