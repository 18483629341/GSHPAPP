  <div class="table_title">
    <span>关联项目</span>
    
  </div>

  <ion-item *ngFor="let item of relativeUnitArr">
    <ion-label text-wrap (click)="changeOption(item.projectCode)">
      {{item.projectName}}
    </ion-label>
  </ion-item>

