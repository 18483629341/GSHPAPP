<ion-header mode='ios'>
  <ion-toolbar mode='ios'>
    <ion-buttons slot="start">
      <ion-back-button text="" icon="ios-arrow-back" mode='ios'></ion-back-button>
    </ion-buttons>
    <ion-title>办理过程</ion-title>
  </ion-toolbar>
</ion-header>

<ion-content>
  <div class="tableBox">
    <table>
      <tr>
        <th>审核流程</th>
        <th>办理人</th>
        <th>办理时间</th>
        <th>办结时间</th>
        <th>意见</th>
      </tr>
      <tr *ngFor="let item of listAarry;let i = index">
        <td>{{item.nodeName?item.nodeName:noData}}</td>
        <td>{{item.transactor?item.transactor:noData}}</td>
        <td>{{item.processTime?item.processTime:noData}}</td>
        <td>{{item.settlTime?item.settlTime:noData}}</td>
        <td>{{item.opinion?item.opinion:noData}}</td>
      </tr>
    </table>
  </div>

  <div *ngFor="let item of delayedList;let i=index">
    <p class="p-title">延时记录{{i+1}}</p>
    <ion-grid class="padding info-color">
      <ion-row>
        <ion-col size="12">
          <span>延时原因：</span>{{item.delayedReason?item.delayedReason:noData}}
        </ion-col>
        <ion-col size="12">
          <span>延时类型：</span>{{item.delayedType?item.delayedType:noData}}
        </ion-col>
        <ion-col size="12">
          <span>开始时间：</span>{{item.startTime?item.startTime:noData}}
        </ion-col>
        <ion-col size="12">
          <span>结束时间：</span>{{item.endTime?item.endTime:noData}}
        </ion-col>
      </ion-row>
    </ion-grid>
  </div>

</ion-content>