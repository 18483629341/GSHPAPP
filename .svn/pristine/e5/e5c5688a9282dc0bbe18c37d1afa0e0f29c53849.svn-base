<ion-header mode='ios'>
    <ion-toolbar mode='ios'>
        <ion-buttons slot="start">
            <ion-back-button text="" icon="ios-arrow-back" mode='ios'></ion-back-button>
        </ion-buttons>
        <ion-title>收藏夹</ion-title>
    </ion-toolbar>
</ion-header>

<ion-content>
    <ion-item lines='none' *ngFor="let item of favoriteList,let i = index">
        <img src="../../../assets/imgs/all.png">
        <ion-label>
            <p>{{item.enterpriseName}}</p>
            <p>{{item.socialCreditCode}}</p>
        </ion-label>
    </ion-item>
</ion-content>