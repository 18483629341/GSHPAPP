ion-content{
    font-size: 14px;
}
ion-col{
    span{
        padding-left: 10px;
    }
}
ion-checkbox{
    --size:16px;
}
.title{
    padding: 12px 0;
}
.checkboxsBox{
    padding: 10px 20px; 

}
 
.border{
    border-bottom: 1px solid #e2e2e2
}
table{
    width: 96%;
    margin:0 auto;
    tr{
         height: 80px;
    }
    td{
         border: 1px solid #e2e2e2
    }
}
.btnBox{
    margin-top: 15px;
    margin-bottom: 10px;
    text-align: center;
}
.btn{
    display: inline-block;
    width: 30%;
    height: 30px;
    line-height: 30px;
    border-radius: 4px;
    background-color: #4162fb;
    color: #fff;
    outline: none;
 
}
.btn+.btn{
    margin-left: 40px;
}
.back{
    background-color: red;
}