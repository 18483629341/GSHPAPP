import { Component, OnInit } from '@angular/core';

@Component({
  selector: 'app-accept-info',
  templateUrl: './accept-info.page.html',
  styleUrls: ['./accept-info.page.scss'],
})
export class AcceptInfoPage implements OnInit {


  isshow: any = false;
  isshow2: any = false;
  isshow3: any = false;
  isshow4: any = false;
  show(val: any) {
    if ( val === 1) {
      this.isshow = !this.isshow;
    } else if ( val === 2) {
      this.isshow2 = !this.isshow2;
    } else if (val === 3) {
      this.isshow3 = !this.isshow3;
    } else if (val === 4) {
      this.isshow4 = !this.isshow4;
    }

  }

  constructor() { }

  ngOnInit() {
  }

}
