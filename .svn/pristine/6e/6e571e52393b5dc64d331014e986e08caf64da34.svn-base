<ion-header mode='ios'>
  <ion-toolbar mode='ios'>
    <ion-buttons slot="start">
      <ion-back-button text="" icon="ios-arrow-back" mode='ios'></ion-back-button>
    </ion-buttons>
    <ion-title>工程师信息</ion-title>
  </ion-toolbar>
</ion-header>


<ion-content>
  <!-- 详情列表 -->
  <ion-grid class="dec-list">
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">姓名：</ion-col>
      <ion-col class="right-text" size="7">{{name}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">身份证号：</ion-col>
      <ion-col class="right-text" size="7">{{IDNumber}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">登记证号：</ion-col>
      <ion-col class="right-text" size="7">{{registrationNumber}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">登记类别：</ion-col>
      <ion-col class="right-text" size="7">{{registrationType}}</ion-col>
    </ion-row>
    <ion-row>
      <ion-col class="left-text" size="6">登记有效期起始日期：</ion-col>
      <ion-col class="right-text" size="6">{{registrationValidityStartTime}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="6">登记有效期终止日期：</ion-col>
      <ion-col class="right-text" size="6">{{registrationValidityEndTime}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">职业资格证书号：</ion-col>
      <ion-col class="right-text" size="7">{{certificateNumber}}</ion-col>
    </ion-row>
    <ion-row align-items-center>

      <ion-col class="left-text" size="5">入职日期：</ion-col>
      <ion-col class="right-text" size="7">{{entryTime}}</ion-col>
    </ion-row>
  </ion-grid>
</ion-content>