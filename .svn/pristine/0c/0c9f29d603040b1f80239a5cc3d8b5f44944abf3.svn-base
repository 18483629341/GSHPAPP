<ion-header mode='ios'>
    <ion-toolbar mode='ios'>
        <ion-buttons slot="start">
            <ion-back-button text="" icon="ios-arrow-back" mode='ios'></ion-back-button>
        </ion-buttons>
        <ion-title>建设项目环评审批基础信息表(1)</ion-title>
    </ion-toolbar>
</ion-header>

<ion-content>
  <ion-grid class="padding info-color">
    <ion-row>
      <ion-col size="12">
        <span>项目名称：</span>{{projectName}}
      </ion-col>
      <ion-col size="12">
        <span>项目代码:</span>{{projectNumber}}
      </ion-col>
      <ion-col size="12">
        <span>国民经济行业类型：</span>{{countIndustryType}}
      </ion-col>
      <ion-col size="12">
        <span>环境影响评价行业类别：</span>{{eviIndustryType}}
      </ion-col>
      <ion-col size="12">
        <span>建设地点中心坐标经度(非线性)：</span>{{constructionSiteLon}}
      </ion-col>
      <ion-col size="12">
        <span>建设地点中心坐标纬度(非线性)：</span>{{constructionSiteLat}}
      </ion-col>
      <ion-col size="12">
        <span>建设性质:</span>{{constructionNature}}
      </ion-col>
      <ion-col size="12">
        <span>环境影响评价文件类别：</span>{{eviFileType}}
      </ion-col>
      <ion-col size="12">
        <span>总投资(万元)：</span>{{totalInvest}}
      </ion-col>
      <ion-col size="12">
        <span>环保投资(万元)：</span>{{enviInvest}}
      </ion-col>
      <ion-col size="12">
        <span>所占比例(%)：</span>{{proportion}}
      </ion-col>
      <ion-col size="12">
        <span>计划开工时间：</span>{{planStartTime}}
      </ion-col>
      <ion-col size="12">
        <span>预计投产时间：</span>{{estimatedTime}}
      </ion-col>
      <ion-col size="12">
        <span>建设地点：</span>{{constructionSite}}
      </ion-col>
      <ion-col size="12">
        <span>项目建设周期(月)：</span>{{constructionCycle}}
      </ion-col>
      <ion-col size="12">
        <span>现有工程排污许可证编号(改建、扩建项目)：</span>{{licenseNumber}}
      </ion-col>
      <ion-col size="12">
        <span>项目申请类别：</span>{{peojectApplyType}}
      </ion-col>
      <ion-col size="12">
        <span>规划环评开展情况：</span>{{isDevople}}
      </ion-col>
      <ion-col size="12">
        <span>规划环评文件名：</span>{{planDocumentName}}
      </ion-col>
      <ion-col size="12">
        <span>规划环评审查机关：</span>{{planExamAuthority}}
      </ion-col>
      <ion-col size="12">
        <span>规划环评审查意见文号：</span>{{planExamNumber}}
      </ion-col>
      <ion-col size="12">
        <span>建设内容：</span>{{constructContent}}
      </ion-col>
      <ion-col size="12">
        <span>建设规模：</span>{{constructScale}}
      </ion-col>
      <ion-col size="12">
        <span>建设地点坐标(线性工程)起点经度：</span>{{startLon}}
      </ion-col>
      <ion-col size="12">
        <span>起点纬度：</span>{{startLat}}
      </ion-col>
      <ion-col size="12">
        <span>终点经度：</span>{{endLon}}
      </ion-col>
      <ion-col size="12">
        <span>终点纬度：</span>{{endLat}}
      </ion-col>
      <ion-col size="12">
        <span>工程长度(千米)：</span>{{engineerLength}}
      </ion-col>
      <ion-col size="12">
        <span>建设单位：</span>{{constructionUnit}}
      </ion-col>
      <ion-col size="12">
        <span>建设单位通讯地址：</span>{{address}}
      </ion-col>
      <ion-col size="12">
        <span>建设单位法人代表：</span>{{legalPerson}}
      </ion-col>
      <ion-col size="12">
        <span>建设单位技术负责人：</span>{{technology}}
      </ion-col>
      <ion-col size="12">
        <span>建设单位同意社会信用代码：</span>{{socialCreditCode}}
      </ion-col>
      <ion-col size="12">
        <span>建设单位联系电话：</span>{{constructionUnitPhone}}
      </ion-col>
      <ion-col size="12">
        <span>评价单位名称：</span>{{evaluatUnitName}}
      </ion-col>
      <ion-col size="12">
        <span>评价单位通讯地址：</span>{{evaluatUnitAddress}}
      </ion-col>
      <ion-col size="12">
        <span>评价单位联系电话：</span>{{evaluatUnitPhone}}
      </ion-col>
      <ion-col size="12">
        <span>评价单位证书编号：</span>{{evaluatUnitNumber}}
      </ion-col>
    </ion-row>
  </ion-grid>
</ion-content>