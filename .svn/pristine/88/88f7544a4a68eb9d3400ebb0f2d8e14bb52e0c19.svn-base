<ion-header mode='ios'>
  <ion-toolbar mode='ios'>
    <ion-buttons slot="start">
      <ion-back-button text="" icon="ios-arrow-back" mode='ios'></ion-back-button>
    </ion-buttons>
    <ion-title>建设项目环境保护申请登记表</ion-title>
  </ion-toolbar>
</ion-header>
<ion-content>
  <ion-grid class="dec-list">
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">项目名称：</ion-col>
      <ion-col class="right-text" size="7">{{projectName}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">建设单位：</ion-col>
      <ion-col class="right-text" size="7">{{constructionUnit}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">负责人：</ion-col>
      <ion-col class="right-text" size="7">{{chargePeople}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">建设地点：</ion-col>
      <ion-col class="right-text" size="7">{{constructionSite}}</ion-col>
    </ion-row>
    <ion-row>
      <ion-col class="left-text" size="5">电话：</ion-col>
      <ion-col class="right-text" size="7">{{phone}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">所属市州：</ion-col>
      <ion-col class="right-text" size="7">{{cityName}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">所属区县：</ion-col>
      <ion-col class="right-text" size="7">{{countyName}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">项目性质：</ion-col>
      <ion-col class="right-text" size="7">{{projectNature}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">主管部门：</ion-col>
      <ion-col class="right-text" size="7">{{competentDepartment}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">总投资(万元)：</ion-col>
      <ion-col class="right-text" size="7">{{totalInvest}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">环保投资(万元)：</ion-col>
      <ion-col class="right-text" size="7">{{enviInvest}}</ion-col>
    </ion-row>
    <ion-row>
      <ion-col class="left-text" size="5">通讯地址：</ion-col>
      <ion-col class="right-text" size="7">{{address}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">邮政编码：</ion-col>
      <ion-col class="right-text" size="7">{{postalCode}}</ion-col>
    </ion-row>
    <ion-row>
      <ion-col class="left-text" size="7">预计投产使用时间：</ion-col>
      <ion-col class="right-text" size="5">{{estimatedTime}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">申请登记人：</ion-col>
      <ion-col class="right-text" size="7">{{applyPeople}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">联系人电话：</ion-col>
      <ion-col class="right-text" size="7">{{phone1}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">占地面积(平方米)：</ion-col>
      <ion-col class="right-text" size="7">{{area}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">年增耗水量(吨)：</ion-col>
      <ion-col class="right-text" size="7">{{yearWater}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">年增耗煤量(吨)：</ion-col>
      <ion-col class="right-text" size="7">{{yearCoal}}</ion-col>
    </ion-row>
    <ion-row>
      <ion-col class="left-text" size="5">排水去向：</ion-col>
      <ion-col class="right-text" size="7">{{drainageDirection}}</ion-col>
    </ion-row>
    <ion-row align-items-center>
      <ion-col class="left-text" size="5">申请日期：</ion-col>
      <ion-col class="right-text" size="7">{{applicationTime}}</ion-col>
    </ion-row>
    <ion-row>
        <ion-col class="left-text" size="7">是否在水源保护区：</ion-col>
        <ion-col class="right-text" size="5">{{isReserve}}</ion-col>
      </ion-row>
      <ion-row align-items-center>
        <ion-col class="left-text" size="5">是否在无煤区：</ion-col>
        <ion-col class="right-text" size="7">{{isCoal}}</ion-col>
      </ion-row>
      <ion-row align-items-center>
        <ion-col class="left-text" size="7">涂料使用总量(吨/年)：</ion-col>
        <ion-col class="right-text" size="5">{{coating}}</ion-col>
      </ion-row>
      <ion-row align-items-center>
        <ion-col class="left-text" size="9">其中低挥发性涂料使用量(吨/年)：</ion-col>
        <ion-col class="right-text" size="3">{{lowCoating}}</ion-col>
      </ion-row>
      <ion-row align-items-center>
        <ion-col class="left-text" size="5">建设地址经度：</ion-col>
        <ion-col class="right-text" size="7">{{constructionSiteLon}}</ion-col>
      </ion-row>
      <ion-row align-items-center>
        <ion-col class="left-text" size="5">建设地址纬度：</ion-col>
        <ion-col class="right-text" size="7">{{constructionSiteLat}}</ion-col>
      </ion-row>
      <ion-row>
        <ion-col class="left-text" size="5">主要产品名称：</ion-col>
        <ion-col class="right-text" size="7">{{mainProducts}}</ion-col>
      </ion-row>
      <ion-row align-items-center>
        <ion-col class="left-text" size="5">主要产品年产量：</ion-col>
        <ion-col class="right-text" size="7">{{mainProductsNumber}}</ion-col>
      </ion-row>
      <ion-row align-items-center>
          <ion-col class="left-text" size="5">主要原材料名称：</ion-col>
          <ion-col class="right-text" size="7">{{rawMaterial}}</ion-col>
        </ion-row>
        <ion-row align-items-center>
          <ion-col class="left-text" size="7">主要原材料年产量：</ion-col>
          <ion-col class="right-text" size="5">{{rawMaterialNumber}}</ion-col>
        </ion-row>
        <ion-row>
          <ion-col class="left-text" size="5">周围环境概况：</ion-col>
          <ion-col class="right-text" size="7">{{surroundEnvironment}}</ion-col>
        </ion-row>
        <ion-row align-items-center>
          <ion-col class="left-text" size="5">备注：</ion-col>
          <ion-col class="right-text" size="7">{{mark}}</ion-col>
        </ion-row>
  </ion-grid>

</ion-content>