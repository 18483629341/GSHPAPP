<ion-header mode='ios'>
  <ion-toolbar mode='ios'>
    <ion-buttons slot="start">
      <ion-back-button text="" icon="ios-arrow-back" mode='ios'></ion-back-button>
    </ion-buttons>
    <ion-title>参与项目</ion-title>
  </ion-toolbar>
</ion-header>
<ion-content>
  <!-- 详情列表 -->
  <ion-grid class="dec-list">
      <ion-row align-items-center>
        <ion-col class="left-text" size="5">项目编号：</ion-col>
        <ion-col class="right-text" size="7">{{projectNumber}}</ion-col>
      </ion-row>
      <ion-row align-items-center>
        <ion-col class="left-text" size="5">项目名称：</ion-col>
        <ion-col class="right-text" size="7">{{projectName}}</ion-col>
      </ion-row>
      <ion-row align-items-center>
        <ion-col class="left-text" size="5">评估日期：</ion-col>
        <ion-col class="right-text" size="7">{{assessmentDate}}</ion-col>
      </ion-row>
      <ion-row align-items-center>
        <ion-col class="left-text" size="5">项目经办人评价：</ion-col>
        <ion-col class="right-text" size="7">{{evaluate}}</ion-col>
      </ion-row>
    </ion-grid>
</ion-content>